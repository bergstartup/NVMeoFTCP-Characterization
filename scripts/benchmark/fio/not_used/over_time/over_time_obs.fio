[global]
filename=/dev/nvme1n1
ioengine=libaio
direct=1
bs=4k
numjobs=1
#time_based
#runtime=30
iodepth=1
ramp_time=2
group_reporting=1

[long-running]
rw=write
per_job_logs=0
write_hist_log=hist
log_hist_msec=10000
write_bw_log=bw
write_lat_log=latency
write_iops_log=iops
steadystate=iops:5%
steadystate_duration=30
steadystate_ramp_time=10
log_avg_msec=10000
