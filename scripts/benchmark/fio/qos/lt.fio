[global]
name=${NAME}
time_based=1
ramp_time=${WARMUP}
runtime=${RUNTIME}
ioengine=io_uring
fixedbufs=1
registerfiles=1
direct=1

[latency]
readwrite=${FOP}
iodepth=1
numjobs=1
bs=4k
hipri=${FPOLL}
prio=${FPRIO}
nice=${FNICE}
filename=${FDEVICE}

[throughput]
numjobs=${BCOUNT}
iodepth=128
readwrite=${BOP}
bs=${BBSIZE}
hipri=0
filename=${BDEVICE}
